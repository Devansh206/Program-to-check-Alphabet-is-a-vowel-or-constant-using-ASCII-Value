#include<stdio.h>
int main()
{
  char ch;
  printf("enter the character");
  scanf("%c",&ch);
  if(ch=='97'||ch=='101'||ch=='105'||ch=='111'||ch=='117'||ch=='65'||ch=='69'||ch=='73'||ch=='76'||ch=='85')
    printf("%c number is a vowel",ch);
   else if((ch>='97'&&ch<='122')||(ch>='65'&&ch<='90')
      printf("%c no is a constant",ch);
    else
     printf("number is not a alphabet");
    return 0;
    }
